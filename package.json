{
  "name": "sass_html_sample",
  "version": "1.0.0",
  "description": "HTML/SASS Code Sample",
  "dependencies": {
    "html5-boilerplate": "^7.1.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {},
  "scripts": {
    "build-css": "node-sass-chokidar scss/ -o css/",
    "watch-css": "npm run build-css && node-sass-chokidar scss/ -o css/ --watch --recursive",
    "start": "npm-run-all -p watch-css",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonathansantini/sass_html_sample.git"
  },
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/jonathansantini/sass_html_sample"
}
